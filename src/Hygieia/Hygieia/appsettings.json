{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Argus",
        "Uri": "http://Argus:5000/healthcheck"
      },
      {
        "Name": "Hermes",
        "Uri": "http://Hermes:80/healthcheck"
      }
    ],
    "EvaluationTimeInSeconds": 20,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "AllowedHosts": "*"
}
